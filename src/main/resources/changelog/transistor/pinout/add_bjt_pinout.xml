<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">

    <changeSet id="add_bjt_transistor_pinout" author="dduda">
        <createTable tableName="bjt_transistor_pinout">
            <column name="id" type="bigserial" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="unique_id" type="varchar(36)">
                <constraints unique="true" nullable="false"/>
            </column>
            <column name="pin1" type="varchar(12)">
                <constraints unique="false" nullable="false"/>
            </column>
            <column name="pin2" type="varchar(12)">
                <constraints unique="false" nullable="false"/>
            </column>
            <column name="pin3" type="varchar(12)">
                <constraints unique="false" nullable="false"/>
            </column>
            <column name="transistor" type="varchar(36)">
                <constraints unique="true" nullable="false"/>
            </column>
        </createTable>
        <createSequence sequenceName="bjt_transistor_pinout_seq"/>
    </changeSet>
</databaseChangeLog>