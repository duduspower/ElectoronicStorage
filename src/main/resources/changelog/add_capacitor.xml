<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">

    <changeSet id="add_capacitor" author="dduda">
        <createTable tableName="capacitor">
            <column name="id" type="bigserial" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="unique_id" type="varchar(36)">
                <constraints unique="true" nullable="false"/>
            </column>
            <column name="capacitance" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="multiplier" type="varchar(12)"/>
            <column name="tolerance" type="int"/>
            <column name="power" type="int"/>
            <column name="impedance" type="int"/>
            <column name="capacitor_type" type="varchar(12)"/>
        </createTable>
    </changeSet>
</databaseChangeLog>